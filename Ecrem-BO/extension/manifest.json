{
	"manifest_version": 2,
	"name": "pruebaInternet",
	"description": "Fetches current tab url.",
	"version": "0.2",
	"author": "Alvaro-Duran",
	"browser_action": {
		"default_icon": "icon.png",
		"default_title": "Just observing your current url."
	},
	"permissions": ["tabs", "activeTab", "http://127.0.0.1:5000/*","https://api.ipify.org/", "storage","system.network","geolocation"],
	"background": {
		"scripts": ["background.js"],
		"persistent": false
	}
}